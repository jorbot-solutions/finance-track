{
  "name": "fininace-track",
  "version": "1.0.0",
  "description": "finance track",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"FinanceTrack\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "jorbot",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.8.2",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "electron-packager": "^11.0.1"
  }
}
